#include <iostream>
#include <cmath>
using namespace std;

float dist(float x1, float y1, float x2, float y2) {
    float dx = x2 - x1;
    float dy = y2 - y1;

    return sqrt(pow(dx, 2) + pow (dy, 2));
}
int main() {
    float x1, y1, x2, y2;
    cin >> x1 >> y1 >> x2 >> y2;

    cout << dist(x1, y1, x2, y2) << endl;
}
